  "bugs": {
    "url": "https://github.com/programmingstandards/advanced-object/issues"
  },
  "homepage": "https://github.com/programmingstandards/advanced-object#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "rollup": "^0.52.3",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.2.6"
  }
